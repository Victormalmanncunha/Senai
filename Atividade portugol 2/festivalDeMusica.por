programa {
  funcao inicio() {
    inteiro quantidadeDePessoas
    inteiro quantidadeDeBanheiros
    inteiro quantidadeDeBares
    escreva("Digite a quantidade de pessoas\n")
    leia(quantidadeDePessoas)
    quantidadeDeBanheiros = quantidadeDePessoas/50
    quantidadeDeBares = quantidadeDePessoas/150
    escreva("A média de banheiros necessários são: ", quantidadeDeBanheiros)
    escreva("\nA média de bares necessários são: ", quantidadeDeBares)
    

  }
}

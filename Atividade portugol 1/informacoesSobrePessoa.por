programa {
  funcao inicio() {
    cadeia nome, endereco
    inteiro idade
    escreva("Digite seu nome.\n")
    leia(nome)
    escreva("Digite sua idade.\n")
    leia(idade)
    escreva("Digite seu endereço.\n")
    leia(endereco)
    escreva("Nome:", nome, "\nIdade:", idade, "\nEndereço:", endereco)
  }
}
